<h1>Create a New Blogger</h1>
<% if flash[:errors] %>
    <ul>
        <% flash[:errors].each do |error| %>
            <li><%= error %></li>
        <% end %>
    </ul>
<% end %>

<%= form_with model: @blogger do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :bio %>
  <%= f.text_field :bio %><br>
  <%= f.label :age %>
  <%= f.number_field :age %><br>


  <br>
 
  <%= f.submit "Create Blogger" %>
<% end %>